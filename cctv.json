{
  "id": "cctv",
  "description": "CCTV plugin",
  "gendevices": 0,
  "single": 1,
  "smart": 1,
  "module": "index.js",
  "params": [
    {
      "name": "token",
      "note": "$Token",
      "type": "string",
      "val": ""
    },
    {
      "name": "wsport",
      "note": "$Wsport",
      "type": "number",
      "val": 8099
    }
  ],
  "sch": {
    "parent": true,
    "scheme": {
      "tools": { "filter": false, "fixed": false, "name": "", "order": -1, "type": "parent", "width": 30 },
      "name": { "filter": true, "fixed": false, "name": "$Name", "order": 1, "type": "STRING", "width": 250 },
      "url": { "filter": true, "fixed": false, "name": "$Url", "order": 2, "type": "STRING", "width": 350 },
      "type": { "filter": true, "fixed": false, "name": "$Type", "order": 3, "type": "STRING", "width": 125 },
      "transport": { "filter": false, "fixed": false, "name": "$Transport", "order": 4, "type": "STRING", "width": 150 }
    },
    "record": {
      "cam": {
        "name": "$Cam",
        "order": 0,
        "toolbar": false,
        "properties": {
          "url": { "name": "$Url", "order": 0, "type": "STRING" },
          "name": { "name": "$Name", "order": 1, "type": "STRING" },
          "txt": { "name": "$Txt", "order": 2, "type": "STRING" },
          "type": { "name": "$Type", "order": 3,  "type": "autocomplete",
            "data": [
              { "id": "rtsp/h264", "name": "RTSP/H264" },
              { "id": "rtsp/mjpeg", "name": "RTSP/MJPEG" },
              { "id": "http/mjpeg", "name": "HTTP/MJPEG" },
              { "id": "http/jpeg", "name": "HTTP/JPEG" }
            ]
          },
          "protocol": { "name": "$Protocol", "order": 4,  "type": "autocomplete", "visible": "(data.type === 'rtsp/h264' || data.type === 'rtsp/mjpeg')",
            "data": [
              { "id": "tcp", "name": "TCP" },
              { "id": "udp", "name": "UDP" }
            ]
          },
          "transport": { "name": "$Transport", "order": 5,  "type": "autocomplete",
            "data": [
              { "id": "ws", "name": "WebSocket" },
              { "id": "p2p", "name": "P2P" }
            ]
          },
          "comment": { "name": "$Comment", "order": 6, "type": "STRING" }
        }
      }
    },
    "default": {
      "url": "rtsp://user:pwd@192.168.0.xxx:port/videoMain",
      "name": "",
      "type": "rtsp/h264",
      "protocol": "udp",
      "transport": "p2p",
      "txt": ""
    },
    "defaultchild": {}
  },
  "chpattern": [],
  "genrules": {},
  "commands": []
}
